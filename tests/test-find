: # simple find
---------------
: write test.c
int var;
: ironout find test.c 6
4 7
: # multiple occurrences
------------------------
: write test.c
int var;
int f() {
	return var;
}
: ironout find test.c 6
4 7
27 30
: # finding in function scopes
------------------------------
: write test.c
int f() {
	int var = 1;
	return var;
}
int g() {
	int var = 1;
	return var;
}
: ironout find test.c 16
15 18
32 35
: # finding enum types
----------------------
: write test.c
int f() {
	enum values {
		value1,
		value2 = 5
	} val1 = value1;
	enum values val2 = value2;
}
int g() {
	int value1, value2, values, val1;
}
: ironout find test.c 20
16 22
72 78
: ironout find test.c 60
27 33
58 64
